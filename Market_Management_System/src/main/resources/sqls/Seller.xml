<mapper namespace="com.sic.pdm.sql.Seller">
<select id="sellerloginchk" parameterType="SellerVo" resultType="java.lang.String">
SELECT SELLERID FROM "SELLER"
WHERE SELLERID = #{sellerid}
AND SELLERPW = #{sellerpw}
</select>

